	"typings": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/Asymmetrik/ngx-leaflet.git"
	},
	"scripts": {
		"build": "npm run compile && npm run bundle",
		"prebuild": "npm run lint",
		"demo": "(webpack-dev-server & npm run watch)",
